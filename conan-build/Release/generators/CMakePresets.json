{
    "version": 3,
    "vendor": {
        "conan": {}
    },
    "cmakeMinimumRequired": {
        "major": 3,
        "minor": 15,
        "patch": 0
    },
    "configurePresets": [
        {
            "name": "conan-build-release",
            "displayName": "'conan-build-release' config",
            "description": "'conan-build-release' configure using 'Unix Makefiles' generator",
            "generator": "Unix Makefiles",
            "cacheVariables": {
                "CMAKE_POLICY_DEFAULT_CMP0091": "NEW",
                "CMAKE_BUILD_TYPE": "Release"
            },
            "toolchainFile": "/app/conan-build/Release/generators/conan_toolchain.cmake",
            "binaryDir": "/app/conan-build/Release"
        }
    ],
    "buildPresets": [
        {
            "name": "conan-build-release",
            "configurePreset": "conan-build-release",
            "jobs": 12
        }
    ],
    "testPresets": [
        {
            "name": "conan-build-release",
            "configurePreset": "conan-build-release"
        }
    ]
}